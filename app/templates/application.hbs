<div class="card-columns">
  {{#each model as |song|}}
    <div class="card">
      <img class="card-img-top" style="width: 100%; height: 160px;" src="{{song.album.largeArtwork}}" alt="{{song.album.name}}">
      <div class="card-block">
        <h4 class="card-title">{{song.name}}</h4>
        <p class="card-text">{{song.artist.name}} - {{song.album.name}}</p>
      </div>
    </div>
  {{/each}}
</div>

{{outlet}}
